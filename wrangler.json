{
  "name": "loftwahfm",
  "compatibility_date": "2025-08-11",
  "compatibility_flags": ["nodejs_compat"],
  "main": "./dist/_worker.js/index.js",
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS"
  },
  "r2_buckets": [
    {
      "binding": "MEDIA",
      "bucket_name": "loftwahfm",
      "preview_bucket_name": "loftwahfm-dev"
    }
  ],
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true
}
